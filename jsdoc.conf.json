{
    "recurseDepth": 10,
    "source": {
        "include":["./src/server/route"],
        "exclude":[],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "plugins": ["plugins/markdown"],
    "markdown": {
        "tags": ["foo"]
    }
}