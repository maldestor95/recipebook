{
    "name": "ludoserver",
    "version": "0.0.1",
    "description": "nodejs server",
    "main": "app",
    "scripts": {
        "start": "node ./bin/www",
        "test": "mocha --watch",
        "test:coverage": "nyc --reporter=text mocha -r mocha.env.js ",
        "test:utils": "mocha --config utils/__tests__/.mocharc.js -r mocha.env.js",
        "test:dev": "mocha  -r mocha.env.js --watch",
        "dev": "set NODE_ENV=development &nodemon ./bin/www",
        "createTable": "node lib/dynamodb/createTable.js",
        "deleteTable": "node lib/dynamodb/deleteTable.js",
        "scanTable": "node lib/dynamodb/scanTable.js",
        "dynamodblocal": "powershell.exe \"./utils/launchdynamodblocal.ps1\""
    },
    "author": "maldestor95",
    "license": "ISC",
    "dependencies": {
        "body-parser": "^1.19.0",
        "connect-dynamodb": "^2.0.4",
        "connect-history-api-fallback": "^1.6.0",
        "express": "^4.17.1",
        "express-session": "^1.17.0",
        "lodash": "^4.17.15",
        "multer": "^1.4.2",
        "multer-s3": "^2.9.0",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "qs": "^6.9.1",
        "uuid": "^7.0.3",
        "validate.js": "^0.13.1"
    },
    "devDependencies": {
        "aws-sdk": "^2.632.0",
        "axios": "^0.19.2",
        "chai": "^4.2.0",
        "concurrently": "^5.3.0",
        "dotenv": "^8.2.0",
        "find-free-port": "^2.0.0",
        "gulp": "^4.0.2",
        "mocha": "^8.1.3",
        "nodemon": "^2.0.2",
        "nyc": "^15.1.0"
    }
}
